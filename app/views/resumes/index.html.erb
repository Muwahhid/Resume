<p id="notice"><%= notice %></p>

<h1>Resumes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Dob</th>
      <th>Email</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Gender</th>
      <th>Qualification</th>
      <th>Experience</th>
      <th>Projects</th>
      <th>Hobbies</th>
      <th>Languagesknown</th>
      <th>School name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resumes.each do |resume| %>
      <tr>
        <td><%= resume.Name %></td>
        <td><%= resume.DOB %></td>
        <td><%= resume.Email %></td>
        <td><%= resume.Address %></td>
        <td><%= resume.Phone %></td>
        <td><%= resume.Gender %></td>
        <td><%= resume.Qualification %></td>
        <td><%= resume.Experience %></td>
        <td><%= resume.Projects %></td>
        <td><%= resume.Hobbies %></td>
        <td><%= resume.LanguagesKnown %></td>
        <td><%= resume.School_name %></td>
        <td><%= link_to 'Show', resume %></td>
        <td><%= link_to 'Edit', edit_resume_path(resume) %></td>
        <td><%= link_to 'Destroy', resume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resume', new_resume_path %>
